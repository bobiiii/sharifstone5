{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    
     "jsx-runtime": {
        "plugins": [
          "react"
        ],
        "parserOptions": {
          "ecmaFeatures": {
            "jsx": true
          },
          "ecmaVersion": "latest",
        "sourceType": "module",
          "jsxPragma": null 
        },
        "rules": {
          "react/react-in-jsx-scope": 0,
          "react/jsx-uses-react'": 0,
          "no-console": "error",
          "react/prop-types": "off"
      
        }
      }
}
